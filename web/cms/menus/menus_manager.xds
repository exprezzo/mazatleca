                    "cn": [
                        {
                            "id": "MyTextField1",
                            "cid": "textfield",
                            "jsClass": "MyTextField1",
                            "snapToGrid": 10,
                            "userConfig": {
                                "fieldLabel": "id",
                                "anchor": "100%",
                                "name": "id",
                                "autoRef": "txtId"
                            }
                        },
                        {
                            "id": "MyTextField",
                            "cid": "textfield",
                            "jsClass": "MyTextField",
                            "snapToGrid": 10,
                            "userConfig": {
                                "fieldLabel": "Nombre",
                                "anchor": "100%",
                                "name": "nombre",
                                "autoRef": "txtNombre"
                            }
                        }
                    ]
                },
                {
