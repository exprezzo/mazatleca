                    "cid": "fieldset",
                    "jsClass": "MyFieldSet",
                    "snapToGrid": 10,
                    "userConfig": {
                        "title": "Menus",
                        "height": 317
                    },
                    "cn": [
                        {
                            "id": "MyGrid",
                            "cid": "grid",
                            "jsClass": "MyGrid",
                            "snapToGrid": 10,
                            "userConfig": {
                                "title": "-ext-undefined-",
                                "store": "stoMenus",
                                "height": 212,
                                "autoExpandColumn": "colNombre",
                                "style": "border-width:1px;"
                            },
                            "cn": [
                                {
                                    "id": "MyColumn",
                                    "cid": "gridcolumn",
                                    "jsClass": "MyColumn",
                                    "snapToGrid": 10,
                                    "userConfig": {
                                        "dataIndex": "string",
                                        "header": "Nombre",
                                        "sortable": true,
                                        "resizable": "-ext-undefined-",
